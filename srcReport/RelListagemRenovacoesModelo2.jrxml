	<queryString>
		<![CDATA[SELECT
	proposta.id as proposta_id,
	proposta.corretora_id as corretora_id,
	corretora.nome as corretora_nome,
	proposta.data_proposta,
	proposta.numero as proposta_numero,
	proposta.inicio_vigencia,
	proposta.termino_vigencia,
	proposta.nro_apolice,
	proposta.situacao_operacional,
	proposta.premio_liquido,
	CASE
		WHEN proposta.status_operacional BETWEEN 1 AND 4 THEN 'A'
		WHEN proposta.status_operacional BETWEEN 5 AND 7 OR proposta.status_operacional = 51 THEN 'E'
		WHEN proposta.status_operacional BETWEEN 8 and 10 OR proposta.status_operacional = 81 THEN 'C'
		ELSE ''
	END AS status_operacional_sigla,
	CASE
		WHEN proposta.situacao_operacional = 0 THEN 'A'
		WHEN proposta.situacao_operacional = 1 THEN 'C'
		WHEN proposta.situacao_operacional = 2 THEN 'E'
		WHEN proposta.situacao_operacional = 3 THEN 'R'
		ELSE ''
	END AS situacao_operacional_sigla,
	cliente.nome AS cliente_nome,
	cliente.cpf_ou_cnpj AS cliente_cpf_cnpj,
	clientePF.data_nascimento,
	clientePF.sexo,
	clientePF.estado_civil,
	ramo.nome as ramo_nome,
	seguradoraPessoa.cpf_ou_cnpj AS seguradora_cpf_cnpj,
	seguradoraPJ.id as seguradora_id,
	seguradoraPJ.razao_social AS seguradora_razao_social,
	produtor.nome AS produtor_nome,
	grupoVenda.nome_do_grupo,
	cliente.telefone_preferencial,
	cliente.telefone_celular,
	proposta.data_transmissao,
	ramo.id as ramo_id,
	CASE ramo.id
		WHEN 2 THEN (
			SELECT CASE WHEN COUNT(1) > 1 THEN '*' ELSE '' END FROM AUTOMOVEL WHERE proposta_id = proposta.id )
		WHEN 7 THEN (
			SELECT CASE WHEN COUNT(1) > 1 THEN '*' ELSE '' END FROM IMOVEL WHERE proposta_id = proposta.id )
		WHEN 8 THEN (
			SELECT CASE WHEN COUNT(1) > 1 THEN '*' ELSE '' END FROM IMOVEL WHERE proposta_id = proposta.id )
		WHEN 9 THEN (
			SELECT CASE WHEN COUNT(1) > 1 THEN '*' ELSE '' END FROM IMOVEL WHERE proposta_id = proposta.id )
	END as frota,
	(SELECT COUNT(1) FROM sinistro WHERE proposta_id = proposta.id) AS qtdsinistro
FROM
	PROPOSTA proposta
	INNER JOIN PESSOA corretora ON proposta.corretora_id = corretora.id
	INNER JOIN CORRETORA dadosCorretora ON dadosCorretora.id = corretora.id
	LEFT JOIN RAMO ramo ON ramo.id = proposta.ramo_id
	LEFT JOIN PESSOA cliente ON proposta.cliente_id = cliente.id
	LEFT JOIN PESSOA_FISICA clientePF ON proposta.cliente_id = clientePF.id
	LEFT JOIN GRUPO_DE_VENDA grupoVenda ON proposta.grupo_de_venda_id = grupoVenda.id
	LEFT JOIN PESSOA produtor on proposta.produtor_id = produtor.id
	LEFT JOIN PESSOA seguradoraPessoa ON seguradoraPessoa.id = proposta.seguradora_id
	LEFT JOIN PESSOA_JURIDICA seguradoraPJ ON seguradoraPJ.id = proposta.seguradora_id
WHERE   (($P{ASSESSORIA} = '-1' OR proposta.corretora_id in (select corretora_id from corretoras_list where usuario_id = $P{USUARIO_ID}) OR dadosCorretora.assessoria_id = $P{CORRETORAS})) AND
	(($P{CORRETORA_ID} = '-1') OR proposta.corretora_id = $P{CORRETORA_ID}) AND
        ((proposta.situacao_operacional IN (0, 3) AND $P{SITUACAO_PROPOSTA} = 0) OR (proposta.situacao_operacional =0 AND $P{SITUACAO_PROPOSTA} = 1)) AND
        ((proposta.status_operacional > 0 AND $P{SITUACAO_PROPOSTA} = 0) OR (proposta.status_operacional IN (2,4,6,7) AND $P{SITUACAO_PROPOSTA} = 1)) AND
	(to_date('1900-01-01','YYYY-MM-DD') = $P{DATA_PROPOSTA_INICIO} OR proposta.data_proposta >= $P{DATA_PROPOSTA_INICIO}) AND
	(to_date('2900-01-01','YYYY-MM-DD') = $P{DATA_PROPOSTA_TERMINO} OR proposta.data_proposta <= $P{DATA_PROPOSTA_TERMINO}) AND
	(to_date('1900-01-01','YYYY-MM-DD') = $P{INICIO_INICIO_VIGENCIA}  OR proposta.inicio_vigencia >= $P{INICIO_INICIO_VIGENCIA}) AND
	(to_date('2900-01-01','YYYY-MM-DD') = $P{FIM_INICIO_VIGENCIA} OR proposta.inicio_vigencia <= $P{FIM_INICIO_VIGENCIA}) AND
	(to_date('1900-01-01','YYYY-MM-DD') = $P{INICIO_TERMINO_VIGENCIA}  OR proposta.termino_vigencia >= $P{INICIO_TERMINO_VIGENCIA}) AND
	(to_date('2900-01-01','YYYY-MM-DD') = $P{FIM_TERMINO_VIGENCIA} OR proposta.termino_vigencia <= $P{FIM_TERMINO_VIGENCIA}) AND
	(to_date('1900-01-01','YYYY-MM-DD') = $P{DATA_INICIO} OR proposta.data_transmissao >= $P{DATA_INICIO}) AND
	(to_date('2900-01-01','YYYY-MM-DD') = $P{DATA_TERMINO} OR proposta.data_transmissao <= $P{DATA_TERMINO}) AND
	(($P{SEGURADORA_ID} = '-1') OR proposta.seguradora_id = $P{SEGURADORA_ID}) AND
	(($P{RAMO_ID}  = '-1') OR ramo.id = $P{RAMO_ID}) AND
	(($P{PRODUTOR_ID} = '-1') OR proposta.produtor_id = $P{PRODUTOR_ID}) AND
	(($P{CLIENTE_ID} ='-1' ) OR cliente.id = $P{CLIENTE_ID}) AND
	(($P{GRUPO_VENDA_ID} = '-1') OR proposta.grupo_de_venda_id = $P{GRUPO_VENDA_ID})]]>
	</queryString>
