	<queryString>
		<![CDATA[SELECT
	proposta.corretora_id as corretora_id,
	corretora.nome as corretora_nome,
	seguradora.id as seguradora_id,
	grupoVenda.NOME_DO_GRUPO,
	repasse.nro_da_parcela AS nro_da_parcela,
	repasse.data_pagamento AS data_pagamento,
	repasse.data_lancamento AS data_lancamento,
	repasse.data_lancamento AS data_extrato,
	repasse.situacao AS situacao,
	produtor.nome AS produtor_nome,
	ramo.id	AS ramo_id,
	ramo.nome AS ramo_nome,
	pessoa_juridica.razao_social AS seguradora_razao_social,
	cliente.nome AS cliente_nome,
	proposta.numero AS proposta_numero,
	proposta.nro_apolice AS numero_apolice,
	proposta.inicio_vigencia AS inicio_vigencia,
	proposta.termino_vigencia AS termino_vigencia,
	proposta.qtde_parcelas AS qtde_parcelas,
	proposta.parc_receber_comissao AS parcelas_comissao,
	proposta.valor_comissao AS valor_comissao,
	comissionado.comissao AS perc_comissao,
	NULL AS agenciado_comissao,
	NULL as comissionado_nome,
	NULL as agenciado_nome,
	NULL AS referencia_parcela,
	repasse.valor_pago AS valor_repasse,
	repasse.valor_base as valor_base,
	repasse.percentual_repasse as percentual_repasse,
	proposta.is_fatura as is_pagto_via_fatura
FROM
	comissionado_proposta comissionado
	INNER JOIN repasse_comissao repasse ON comissionado.id = repasse.comissionado_id
	INNER JOIN proposta proposta ON comissionado.proposta_id = proposta.id
	INNER JOIN pessoa produtor ON comissionado.pessoa_id = produtor.id
	INNER JOIN ramo ramo ON proposta.ramo_id = ramo.id
	INNER JOIN pessoa_juridica pessoa_juridica ON proposta.seguradora_id = pessoa_juridica.id
	INNER JOIN pessoa cliente ON proposta.cliente_id = cliente.id
	INNER JOIN seguradora seguradora ON pessoa_juridica.id = seguradora.id
	INNER JOIN GRUPO_DE_VENDA grupoVenda ON proposta.grupo_de_venda_id = grupoVenda.id
	LEFT JOIN PESSOA corretora ON proposta.corretora_id = corretora.id
WHERE
	(proposta.is_fatura = false) and
	(($P{CORRETORAS} = '-1') OR (proposta.corretora_id in (select corretora_id from corretoras_list where usuario_id = $P{USUARIO_ID}))) AND
	(($P{CORRETORA_ID} = '-1') OR (proposta.corretora_id = $P{CORRETORA_ID})) AND
	(($P{SITUACAO_COMISSAO} = 0) OR (to_date('1900-01-01','YYYY-MM-DD') = $P{INICIO_DATA_LANCAMENTO}) OR (repasse.data_lancamento >= $P{INICIO_DATA_LANCAMENTO})) AND
	((to_date('2900-01-01','YYYY-MM-DD') = $P{FIM_DATA_LANCAMENTO}) OR (repasse.data_lancamento <= $P{FIM_DATA_LANCAMENTO})) AND
	((to_date('1900-01-01','YYYY-MM-DD') = $P{INICIO_DATA_PAGAMENTO}) OR (repasse.data_pagamento >= $P{INICIO_DATA_PAGAMENTO})) AND
	((to_date('2900-01-01','YYYY-MM-DD') = $P{FIM_DATA_PAGAMENTO}) OR (repasse.data_pagamento <= $P{FIM_DATA_PAGAMENTO})) AND
	(($P{SEGURADORA_ID} = '-1') OR (proposta.seguradora_id = $P{SEGURADORA_ID})) AND
	(($P{RAMO_ID}  = '-1') OR (ramo.id = $P{RAMO_ID})) AND
	(($P{PRODUTOR_ID} = '-1') OR (produtor.id = $P{PRODUTOR_ID})) AND
	(($P{CLIENTE_ID} ='-1' ) OR (cliente.id = $P{CLIENTE_ID})) AND
	(($P{SITUACAO_COMISSAO} = '-1') OR (repasse.situacao = $P{SITUACAO_COMISSAO})) AND
	(($P{GRUPO_VENDA_ID} = '-1') OR (proposta.grupo_de_venda_id = $P{GRUPO_VENDA_ID}))
UNION ALL
SELECT
	proposta.corretora_id as corretora_id,
	corretora.nome as corretora_nome,
	seguradora.id as seguradora_id,
	grupoVenda.NOME_DO_GRUPO,
	repasse.nro_da_parcela AS nro_da_parcela,
	repasse.data_pagamento AS data_pagamento,
	repasse.data_lancamento AS data_lancamento,
	repasse.data_lancamento AS data_extrato,
	repasse.situacao AS situacao,
	CASE
		WHEN repasse.comissionado_id IS NOT NULL THEN prod_comissionado.nome
		WHEN repasse.agenciado_id IS NOT NULL THEN prod_agenciado.nome
		ELSE 'Indefinido'
	END AS produtor_nome,
	ramo.id	AS ramo_id,
	ramo.nome AS ramo_nome,
	pessoa_juridica.razao_social AS seguradora_razao_social,
	cliente.nome AS cliente_nome,
	proposta.numero AS proposta_numero,
	proposta.nro_apolice AS numero_apolice,
	proposta.inicio_vigencia AS inicio_vigencia,
	proposta.termino_vigencia AS termino_vigencia,
	proposta.qtde_parcelas AS qtde_parcelas,
	proposta.parc_receber_comissao AS parcelas_comissao,
	proposta.valor_comissao AS valor_comissao,
	comissionado.comissao AS comissionado_comissao,
	agenciado.comissao AS agenciado_comissao,
	prod_comissionado.nome as comissionado_nome,
	prod_agenciado.nome as agenciado_nome,
	repasse.referencia_parcela AS referencia_parcela,
	repasse.valor_pago AS valor_repasse,
	repasse.valor_base as valor_base,
	repasse.percentual_repasse as percentual_repasse,
	proposta.is_fatura as is_pagto_via_fatura
FROM
     repasse_comissao repasse
     LEFT OUTER JOIN comissionado_proposta comissionado ON comissionado.id = repasse.comissionado_id
     LEFT OUTER JOIN comissionado_agenciamento agenciado ON agenciado.id = repasse.agenciado_id
     LEFT OUTER JOIN proposta proposta ON comissionado.proposta_id = proposta.id or agenciado.proposta_id = proposta.id
     LEFT OUTER JOIN pessoa prod_comissionado ON comissionado.pessoa_id = prod_comissionado.id
     LEFT OUTER JOIN pessoa prod_agenciado ON agenciado.pessoa_id = prod_agenciado.id
     INNER JOIN ramo ramo ON proposta.ramo_id = ramo.id
     INNER JOIN pessoa_juridica pessoa_juridica ON proposta.seguradora_id = pessoa_juridica.id
     INNER JOIN pessoa cliente ON proposta.cliente_id = cliente.id
     INNER JOIN seguradora seguradora ON pessoa_juridica.id = seguradora.id
     INNER JOIN GRUPO_DE_VENDA grupoVenda ON proposta.grupo_de_venda_id = grupoVenda.id
     LEFT JOIN PESSOA corretora ON proposta.corretora_id = corretora.id
WHERE
	(proposta.is_fatura = true) AND
	(($P{CORRETORAS} = '-1') OR (proposta.corretora_id in (select corretora_id from corretoras_list where usuario_id = $P{USUARIO_ID}))) AND
	(($P{CORRETORA_ID} = '-1') OR (proposta.corretora_id = $P{CORRETORA_ID})) AND
	(($P{SITUACAO_COMISSAO} = 0) OR (to_date('1900-01-01','YYYY-MM-DD') = $P{INICIO_DATA_LANCAMENTO}) OR (repasse.data_lancamento >= $P{INICIO_DATA_LANCAMENTO})) AND
	((to_date('2900-01-01','YYYY-MM-DD') = $P{FIM_DATA_LANCAMENTO}) OR (repasse.data_lancamento <= $P{FIM_DATA_LANCAMENTO})) AND
	((to_date('1900-01-01','YYYY-MM-DD') = $P{INICIO_DATA_PAGAMENTO}) OR (repasse.data_pagamento >= $P{INICIO_DATA_PAGAMENTO})) AND
	((to_date('2900-01-01','YYYY-MM-DD') = $P{FIM_DATA_PAGAMENTO}) OR (repasse.data_pagamento <= $P{FIM_DATA_PAGAMENTO})) AND
	(($P{SEGURADORA_ID} = '-1') OR (proposta.seguradora_id = $P{SEGURADORA_ID})) AND
	(($P{RAMO_ID}  = '-1') OR (ramo.id = $P{RAMO_ID})) AND
	(($P{PRODUTOR_ID} = '-1') OR (prod_comissionado.id = $P{PRODUTOR_ID}) OR (prod_agenciado.id = $P{PRODUTOR_ID})) AND
	(($P{CLIENTE_ID} ='-1' ) OR (cliente.id = $P{CLIENTE_ID})) AND
	(($P{SITUACAO_COMISSAO} = '-1') OR (repasse.situacao = $P{SITUACAO_COMISSAO})) AND
	(($P{GRUPO_VENDA_ID} = '-1') OR (proposta.grupo_de_venda_id = $P{GRUPO_VENDA_ID}))
UNION ALL
SELECT
	proposta.corretora_id as corretora_id,
	corretora.nome as corretora_nome,
	seguradora.id as seguradora_id,
	grupoVenda.NOME_DO_GRUPO,
	repasse.nro_da_parcela AS nro_da_parcela,
	repasse.data_pagamento AS data_pagamento,
	repasse.data_lancamento AS data_lancamento,
	repasse.data_lancamento AS data_extrato,
	repasse.situacao AS situacao,
	prod_pessoa.nome AS produtor_nome,
	ramo.id	AS ramo_id,
	ramo.nome AS ramo_nome,
	pessoa_juridica.razao_social AS seguradora_razao_social,
	repasse.descricao AS cliente_nome,
	proposta.numero AS proposta_numero,
	proposta.nro_apolice AS numero_apolice,
	proposta.inicio_vigencia AS inicio_vigencia,
	proposta.termino_vigencia AS termino_vigencia,
	proposta.qtde_parcelas AS qtde_parcelas,
	proposta.parc_receber_comissao AS parcelas_comissao,
	proposta.valor_comissao AS valor_comissao,
	comissionado.comissao AS comissionado_comissao,
	agenciado.comissao AS agenciado_comissao,
	prod_comissionado.nome as comissionado_nome,
	prod_agenciado.nome as agenciado_nome,
	repasse.referencia_parcela AS referencia_parcela,
	repasse.valor_pago AS valor_repasse,
	repasse.valor_base as valor_base,
	repasse.percentual_repasse as percentual_repasse,
	proposta.is_fatura as is_pagto_via_fatura
FROM
     repasse_comissao repasse
     LEFT OUTER JOIN comissionado_proposta comissionado ON comissionado.id = repasse.comissionado_id
     LEFT OUTER JOIN comissionado_agenciamento agenciado ON agenciado.id = repasse.agenciado_id
     LEFT OUTER JOIN proposta proposta ON comissionado.proposta_id = proposta.id or agenciado.proposta_id = proposta.id
     LEFT OUTER JOIN pessoa prod_comissionado ON comissionado.pessoa_id = prod_comissionado.id
     LEFT OUTER JOIN pessoa prod_agenciado ON agenciado.pessoa_id = prod_agenciado.id
     LEFT OUTER JOIN produtor produtor ON produtor.id = repasse.produtor_id
     LEFT OUTER JOIN pessoa prod_pessoa ON produtor.id = prod_pessoa.id
     LEFT OUTER JOIN ramo ramo ON proposta.ramo_id = ramo.id
     LEFT OUTER JOIN pessoa_juridica pessoa_juridica ON proposta.seguradora_id = pessoa_juridica.id
     LEFT OUTER JOIN pessoa cliente ON proposta.cliente_id = cliente.id
     LEFT OUTER JOIN seguradora seguradora ON pessoa_juridica.id = seguradora.id
     LEFT OUTER JOIN GRUPO_DE_VENDA grupoVenda ON proposta.grupo_de_venda_id = grupoVenda.id
     LEFT JOIN PESSOA corretora ON proposta.corretora_id = corretora.id
WHERE     (repasse.situacao < 4) AND
	(repasse.origem_lancamento = 1) AND
	(($P{CORRETORAS} = '-1') OR (repasse.corretora_id in (select corretora_id from corretoras_list where usuario_id = $P{USUARIO_ID}))) AND
	(($P{CORRETORA_ID} = '-1') OR (repasse.corretora_id = $P{CORRETORA_ID})) AND
	(($P{SITUACAO_COMISSAO} = '0') OR (to_date('1900-01-01','YYYY-MM-DD') = $P{INICIO_DATA_LANCAMENTO}) OR (repasse.data_lancamento >= $P{INICIO_DATA_LANCAMENTO})) AND
	((to_date('2900-01-01','YYYY-MM-DD') = $P{FIM_DATA_LANCAMENTO}) OR (repasse.data_lancamento <= $P{FIM_DATA_LANCAMENTO})) AND
	((to_date('1900-01-01','YYYY-MM-DD') = $P{INICIO_DATA_PAGAMENTO}) OR (repasse.data_pagamento >= $P{INICIO_DATA_PAGAMENTO})) AND
	((to_date('2900-01-01','YYYY-MM-DD') = $P{FIM_DATA_PAGAMENTO}) OR (repasse.data_pagamento <= $P{FIM_DATA_PAGAMENTO})) AND
	(($P{PRODUTOR_ID} = '-1') OR (produtor.id = $P{PRODUTOR_ID})) AND
	(($P{SITUACAO_COMISSAO} = '-1') OR (repasse.situacao = $P{SITUACAO_COMISSAO}))]]>
