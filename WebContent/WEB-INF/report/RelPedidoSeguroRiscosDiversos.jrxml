	<queryString>
		<![CDATA[SELECT
	case
		when pessoaFisicaCliente.id is not null THEN 'F'
		else 'J'
	end AS "TIPO_PESSOA",

	ramo.nome AS "NOME_RAMO",

	proposta.tipo_proposta AS "TIPO_PROPOSTA",
	proposta.premio_liquido AS "PREMIO_LIQUIDO",
	proposta.premio_total AS "PREMIO_TOTAL",
	proposta.inicio_vigencia AS "INICIO_VIGENCIA",
	proposta.termino_vigencia AS "TERMINO_VIGENCIA",
	proposta.valor_1a_parcela AS "VALOR_1A_PARCELA",
	proposta.numero AS "NUMERO",
	proposta.fator AS "FATOR",
	proposta.cheque_nominal AS "CHEQUE_NOMINAL",
	proposta.forma_pagamento_1a_parcela AS "FORMA_PAGAMENTO_1A_PARCELA",
	proposta.forma_pagamento AS "FORMA_PAGAMENTO",
	proposta.titular_ccorrente AS "TITULAR_CONTA_CORRENTE",
	proposta.observacoes as observacoes,
	proposta.qtde_parcelas AS "PROPOSTA_QTDE_PARCELAS",
	proposta.valor_demais_parcelas AS "VALOR_DEMAIS_PARCELAS",
	proposta.observacoes_complementares AS observacoes_complementares,

	primeiraParcela.nro_cheque AS "NRO_CHEQUE_1A_PARCELA",
	primeiraParcela.nro_banco AS "NRO_BANCO_1A_PARCELA",
	primeiraParcela.codigo_agencia AS "COD_AGENCIA_1A_PARCELA",
	primeiraParcela.conta_corrente AS "CTA_CORRENTE_1A_PARCELA"
FROM PROPOSTA proposta
	LEFT JOIN RAMO ramo ON ramo.id = proposta.ramo_id
	LEFT JOIN RISCO_DIVERSOS risco ON risco.proposta_id = proposta.id
	LEFT JOIN PESSOA_FISICA pessoaFisicaCliente ON pessoaFisicaCliente.id = proposta.cliente_id
	LEFT JOIN PARCELAS_PROPOSTA ultimaParcela ON ultimaParcela.proposta_id = proposta.id
	LEFT JOIN PARCELAS_PROPOSTA primeiraParcela ON primeiraParcela.proposta_id = proposta.id
WHERE
	(risco.id IS NULL OR risco.id = (SELECT MIN(id) FROM RISCO_DIVERSOS WHERE proposta_id = $P{PROPOSTA_ID}))
	AND (primeiraParcela.id IS NULL OR primeiraParcela.id = (SELECT MIN(id) FROM PARCELAS_PROPOSTA WHERE proposta_id = $P{PROPOSTA_ID})) AND
	proposta.id = $P{PROPOSTA_ID}]]>
	</queryString>
